<%= render 'info', user: @user %>

<%= render 'books/new', book: @newbook %>
<h1>Users</h1>

<div>
  <table>
    <tr>
      <td><strong>image</strong></td>
      <td><strong>name</strong></td>
      <td></td>
    </tr>
    <% @users.each do |user| %>
      <tr>
        <td><%= image_tag user.get_profile_image(50, 50) %></td>
        <td><%= user.name %></td>
        <td>
          <%= link_to "Show", user_path(user.id) %>
        </td>
      </tr>
    <% end %>
  </table>
</div>